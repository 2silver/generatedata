{"code":"import { getLocale } from 'client/src/utils/langUtils';\r\nexport var getFormattedNum = function (num, locale) {\r\n    if (locale === void 0) { locale = getLocale(); }\r\n    return Intl.NumberFormat(locale).format(num);\r\n};\r\n// @ts-ignore\r\nexport var isNumeric = function (n) { return !isNaN(parseFloat(n)) && isFinite(n); };\r\n// based on a JS port of the PHP number_format() method: https://locutus.io/php/strings/number_format/\r\nexport var numberFormat = function (number, decimals, decPoint, thousandsSep) {\r\n    if (decimals === void 0) { decimals = 0; }\r\n    if (decPoint === void 0) { decPoint = '.'; }\r\n    if (thousandsSep === void 0) { thousandsSep = ','; }\r\n    var n = !isFinite(+number) ? 0 : +number;\r\n    var prec = !isFinite(+decimals) ? 0 : Math.abs(decimals);\r\n    var sep = (typeof thousandsSep === 'undefined') ? ',' : thousandsSep;\r\n    var dec = (typeof decPoint === 'undefined') ? '.' : decPoint;\r\n    var toFixedFix = function (num, precision) {\r\n        if (('' + num).indexOf('e') === -1) {\r\n            return +(Math.round(Number(n + 'e+' + precision)) + 'e-' + precision);\r\n        }\r\n        else {\r\n            var arr = ('' + n).split('e');\r\n            var sig = '';\r\n            if (+arr[1] + prec > 0) {\r\n                sig = '+';\r\n            }\r\n            return (+(Math.round(Number(+arr[0] + 'e' + sig + (+arr[1] + precision))) + 'e-' + precision)).toFixed(precision);\r\n        }\r\n    };\r\n    var s = (prec ? toFixedFix(n, prec).toString() : '' + Math.round(n)).split('.');\r\n    if (s[0].length > 3) {\r\n        s[0] = s[0].replace(/\\B(?=(?:\\d{3})+(?!\\d))/g, sep);\r\n    }\r\n    if ((s[1] || '').length < prec) {\r\n        s[1] = s[1] || '';\r\n        s[1] += new Array(prec - s[1].length + 1).join('0');\r\n    }\r\n    return s.join(dec);\r\n};\r\n//# sourceMappingURL=numberUtils.js.map","references":["/Users/benkeen/generatedata/client/types/general.d.ts","/Users/benkeen/generatedat